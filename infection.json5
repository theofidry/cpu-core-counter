{
    "$schema": "https://raw.githubusercontent.com/infection/infection/0.26.16/resources/schema.json",
    "ignoreMsiWithNoMutations": true,
    "logs": {
        "text": ".build/infection/infection-log.txt"
    },
    "minCoveredMsi": 100,
    "minMsi": 100,
    "source": {
        "directories": [
            "src"
        ]
    },
    "timeout": 10,

    "mutators": {
        "@default": true,
        "ArrayItemRemoval": {
            "ignore": [
                "Fidry\\CpuCoreCounter\\CpuCoreCounter::getDefaultFinders"
            ]
        },
        "FalseValue": {
            "ignore": [
                // This is from thecodingmachine/safe – leave it alone
                "Fidry\\CpuCoreCounter\\Exec\\ShellExec::execute"
            ]
        },
        "FunctionCallRemoval": {
            "ignore": [
                // This is from thecodingmachine/safe – leave it alone
                "Fidry\\CpuCoreCounter\\Exec\\ShellExec::execute"
            ]
        },
        "PublicVisibility": false
    }
}
