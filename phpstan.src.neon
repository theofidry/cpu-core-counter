parameters:
	inferPrivatePropertyTypeFromConstructor: true

	level: max

	paths:
		- src

	tmpDir: .build/phpstan/src/

	ignoreErrors:

		- path: src/Finder/EnvVariableFinder.php
		  message: '#find\(\) should return int\<1\, max\>\|null but returns int\|null\.#'

		- path: src/ParallelisationResult.php
		  message: '#CpuCoreCounter::getAvailableForParallelisation\(\) should return int\<1\, max\> but returns int\.#'
